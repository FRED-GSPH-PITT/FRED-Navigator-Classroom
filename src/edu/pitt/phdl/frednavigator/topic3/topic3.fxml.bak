            <Tab id="tab-SEIR" fx:id="tabSEIR" closable="false" style="" text="Explore Health State">
              <content>
                <BorderPane fx:id="brdrPaneSEIR" styleClass="graph-tab-pane-text-region">
                  <top>
                    <AnchorPane prefHeight="200.0" prefWidth="200.0">
